<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="#ff16a085" />
            <stroke android:width="1.0px" android:color="#ffffffff" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#4116a085" />
            <stroke android:width="1.0px" android:color="#ffffffff" />
            <corners android:radius="6.0px" />
        </shape>
    </item>
</selector>