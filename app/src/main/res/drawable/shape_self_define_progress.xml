<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">//定义的是背景色
        <shape>
            <corners android:radius="6.0dip" />
            <solid android:color="#ff94d3c7" />
        </shape>
    </item>
    <item android:id="@android:id/progress">//这个是定义1级进度，如果1级进度比2级进度大且1级进度不透明，就可以覆盖掉2级进度
        <clip>
            <shape>
                <corners android:radius="6.0dip" />
                <solid android:color="#ff16a085" />
            </shape>
        </clip>
    </item>
</layer-list>