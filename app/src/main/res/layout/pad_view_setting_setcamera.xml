<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/settingBg"
    android:paddingLeft="20.0dip"
    android:paddingTop="20.0dip"
    android:paddingRight="20.0dip"
    android:paddingBottom="20.0dip"
    android:scrollbars="none"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/llCameraExposure"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:background="@drawable/shape_setting_selector_top"
            android:clickable="true"
            android:orientation="horizontal"
            android:paddingLeft="30.0dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:drawableLeft="@drawable/img_module_tab_setting_base_verification"
                android:drawablePadding="8.0dip"
                android:gravity="center_vertical"
                android:state_pressed="true"
                android:text="曝光度      "
                android:textSize="20.0sp" />

            <TextView
                android:id="@+id/tvCameraExposure"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:text="0"
                android:textSize="20.0sp" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="#ffdcdcdc" />

        <LinearLayout
            android:id="@+id/llCameraCollectQuality"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:background="@drawable/shape_setting_selector_center"
            android:clickable="true"
            android:orientation="horizontal"
            android:paddingLeft="30.0dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:drawableLeft="@drawable/img_module_tab_setting_base_verification"
                android:drawablePadding="8.0dip"
                android:gravity="center_vertical"
                android:state_pressed="true"
                android:text="采集质量      "
                android:textSize="20.0sp" />

            <TextView
                android:id="@+id/tvCameraCollectQuality"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:text="0"
                android:textSize="20.0sp" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="#ffdcdcdc" />

        <LinearLayout
            android:id="@+id/llCameraComparisonThreshold"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:background="@drawable/shape_setting_selector_center"
            android:clickable="true"
            android:orientation="horizontal"
            android:paddingLeft="30.0dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:drawableLeft="@drawable/img_module_tab_setting_base_verification"
                android:drawablePadding="8.0dip"
                android:gravity="center_vertical"
                android:state_pressed="true"
                android:text="比对阈值      "
                android:textSize="20.0sp" />

            <TextView
                android:id="@+id/tvCameraComparisonThreshold"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:text="60%"
                android:textSize="20.0sp" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="#ffdcdcdc" />

        <LinearLayout
            android:id="@+id/llCameraDirection"
            android:layout_width="fill_parent"
            android:layout_height="50.0dip"
            android:background="@drawable/shape_setting_selector_center"
            android:clickable="true"
            android:orientation="horizontal"
            android:paddingLeft="30.0dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:drawableLeft="@drawable/img_module_tab_setting_base_verification"
                android:drawablePadding="8.0dip"
                android:gravity="center_vertical"
                android:state_pressed="true"
                android:text="摄像头      "
                android:textSize="20.0sp" />

            <TextView
                android:id="@+id/tvCameraDirection"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:text="正"
                android:textSize="20.0sp" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>